    {
      "metadata": {
        "id": "grDkqVz-XhDC",
        "colab_type": "code",
        "colab": {}
      },
      "cell_type": "code",
      "source": [
        "# Some of the states have space after the name\n",
        "state_insurance[\"State\"]=state_insurance[\"State\"].str.rstrip( )\n",
        "\n",
        "# I also need to isolate the numbers, since the % signs made it a string\n",
        "state_insurance[\"Uninsured Rate (2010)\"]=state_insurance[\"Uninsured Rate (2010)\"].str.split('%').apply(lambda x: x[0])\n",
        "state_insurance[\"Uninsured Rate (2015)\"]=state_insurance[\"Uninsured Rate (2015)\"].str.split('%').apply(lambda x: x[0])\n",
        "state_insurance[\"Uninsured Rate Change (2010-2015)\"]=state_insurance[\"Uninsured Rate Change (2010-2015)\"].str.split('%').apply(lambda x: x[0])"
      ],
      "execution_count": 0,
      "outputs": []
    },
